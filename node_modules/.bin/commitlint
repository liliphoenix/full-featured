#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bobi/code/full-featured/node_modules/.pnpm/@commitlint+cli@19.2.0_@types+node@20.11.28_typescript@5.4.2/node_modules/@commitlint/cli/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/@commitlint+cli@19.2.0_@types+node@20.11.28_typescript@5.4.2/node_modules/@commitlint/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/@commitlint+cli@19.2.0_@types+node@20.11.28_typescript@5.4.2/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bobi/code/full-featured/node_modules/.pnpm/@commitlint+cli@19.2.0_@types+node@20.11.28_typescript@5.4.2/node_modules/@commitlint/cli/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/@commitlint+cli@19.2.0_@types+node@20.11.28_typescript@5.4.2/node_modules/@commitlint/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/@commitlint+cli@19.2.0_@types+node@20.11.28_typescript@5.4.2/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@commitlint/cli/cli.js" "$@"
else
  exec node  "$basedir/../@commitlint/cli/cli.js" "$@"
fi
