#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bobi/code/full-featured/node_modules/.pnpm/cz-customizable@7.0.0/node_modules/cz-customizable/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/cz-customizable@7.0.0/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bobi/code/full-featured/node_modules/.pnpm/cz-customizable@7.0.0/node_modules/cz-customizable/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/cz-customizable@7.0.0/node_modules:/Users/bobi/code/full-featured/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../standalone.js" "$@"
else
  exec node  "$basedir/../../standalone.js" "$@"
fi
