#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shareit/shareit-coll/full-featured/packages/cli/node_modules:/Users/shareit/shareit-coll/full-featured/packages/node_modules:/Users/shareit/shareit-coll/full-featured/node_modules:/Users/shareit/shareit-coll/node_modules:/Users/shareit/node_modules:/Users/node_modules:/node_modules:/Users/shareit/Library/pnpm/global/5/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shareit/shareit-coll/full-featured/packages/cli/node_modules:/Users/shareit/shareit-coll/full-featured/packages/node_modules:/Users/shareit/shareit-coll/full-featured/node_modules:/Users/shareit/shareit-coll/node_modules:/Users/shareit/node_modules:/Users/node_modules:/node_modules:/Users/shareit/Library/pnpm/global/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../Library/pnpm/global/5/node_modules/full-featured-cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../../Library/pnpm/global/5/node_modules/full-featured-cli/index.js" "$@"
fi
