{
  "name": "vite-react",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint ./src --ext .ts,.tsx,.jsx,.js --fix",
